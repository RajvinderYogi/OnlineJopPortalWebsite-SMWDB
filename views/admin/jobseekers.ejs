<% include ../shared/header.ejs %>
<%if (user.userType == "admin"){%>
<div class="container">
<h1 style="text-align: center; font-weight: bold; padding: 20px">All Job Seekers</h1>
<table class="table table-striped table-bordered table-hover">
    <caption>All Job Seekers List</caption>
    <tr>
        <th>Username</th>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    <% for(i=0; i<jobseeker.length; i++) { %>
    <tr>
        <td><%= jobseeker[i].username %></td>
        <td><%= jobseeker[i].fName %></td>
        <td><%= jobseeker[i].lName %></td>
    </tr>
    <%}%>
</table>
<%}
else{%>
<h1> Sorry You do not have Permission to access this page.</h1>
<%}%>
</div>
<% include ../shared/footer.ejs %>
