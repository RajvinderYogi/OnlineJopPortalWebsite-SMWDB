
<% include ../shared/header.ejs %>
<%if (user.userType == "admin"){%>
<table class="table table-striped table-hover">
    <tr>
        <th>Event</th>
        <th>Delete</th>
    </tr>
    <% for(i=0; i<events.length; i++) { %>
    <tr >
        <td><%= events[i].eventName %></td>
        <td>
            <a href="/events/delete/<%= events[i]._id%>" class="btn btn-danger col-md-3" onclick="return confirm('Are you sure want to delete this Event?')"> Delete Event</a></td>
    </tr>
    <%}%>
</table>
<%}
else{%>
<h1> Sorry You do not have Permission to access this page.</h1>
<%}%>
<% include ../shared/footer.ejs %>